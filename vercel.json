{
    "buildCommand": "bun run build",
    "installCommand": "bun install",
    "framework": "other",
    "outputDirectory": ".",
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/index.ts"
      },
      {
        "source": "/(.*)",
        "destination": "/api/index.ts"
      }
    ],
    "functions": {
      "api/index.ts": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }